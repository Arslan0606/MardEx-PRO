<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:dither="true"
            android:shape="rectangle"
            android:visible="true">
            <corners android:radius="@dimen/_50sdp" />
            <size
                android:width="@dimen/_160sdp"
                android:height="@dimen/_50sdp" />
            <stroke
                android:width="@dimen/_1sdp"
                android:color="@color/light_green" />
        </shape>
    </item>

    <item
        android:bottom="@dimen/_5sdp"
        android:left="@dimen/_5sdp"
        android:right="@dimen/_5sdp"
        android:top="@dimen/_5sdp">
        <shape
            android:shape="rectangle"
            android:visible="true">
            <corners android:radius="@dimen/_50sdp" />
            <solid android:color="@color/new_green" />
            <stroke
                android:width="@dimen/_2sdp"
                android:color="@color/free_button_border" />
            <size
                android:width="@dimen/_160sdp"
                android:height="@dimen/_50sdp" />
        </shape>
    </item>

</layer-list>